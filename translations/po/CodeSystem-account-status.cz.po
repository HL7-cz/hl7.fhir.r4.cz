# en -> cs
msgctxt "active"
msgid "Active"
msgstr "aktivní"

msgctxt "active@definition"
msgid "This account is active and may be used."
msgstr "Tento účet je aktivní a může být použit."

msgctxt "inactive"
msgid "Inactive"
msgstr "neaktivní"

msgctxt "inactive@definition"
msgid "This account is inactive and should not be used to track financial information."
msgstr ""
"Tento účet je neaktivní a neměl by být používán ke sledování finančních "
"informací."

msgctxt "entered-in-error"
msgid "Entered in error"
msgstr "zadáno chybně"

msgctxt "entered-in-error@definition"
msgid "This instance should not have been part of this patient's medical record."
msgstr "Tento údaj neměl být součástí zdravotnické dokumentace tohoto pacienta."

msgctxt "on-hold"
msgid "On Hold"
msgstr "pozastaven"

msgctxt "on-hold@definition"
msgid "This account is on hold."
msgstr "Tento účet je pozastaven."

msgctxt "unknown"
msgid "Unknown"
msgstr "neznámý"

msgctxt "unknown@definition"
msgid "The account status is unknown."
msgstr "Stav účtu není znám."
